Addition Vs Concationation
// When both is literals
var x = 5;
var y = 6;
console.log(x + y); //11

// When both is string
var x = "5";
var y = "6";
console.log(x + y); // 56

// When one is literals & other one is string
// Addition
var x = 5;
var y = "6";
console.log(x + y); // 56

// Multiplication
var x = 5;
var y = "6";
console.log(x * y); // NaN  -- Not a number
